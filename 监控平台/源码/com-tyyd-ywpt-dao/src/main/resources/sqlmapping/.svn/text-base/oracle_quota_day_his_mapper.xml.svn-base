<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tyyd.ywpt.dao.core.collect.his.oracle.dataobject.OracleQuotaDayHisDomain" >

  <insert id="move_his_data" parameterType="java.util.Map" >
    insert into b_oracle_quota_day_his ( 
    	quota_collect_id, 
      	host_id, 
      	db_id, 
      	quota_id, 
      	quota_name, 
      	quota_value, 
      	last_quota_value,
      	last_quota_date, 
      	gmt_created, 
      	gmt_modifed) 
     select id, 
    		host_id, 
    		db_id, 
    		quota_id, 
    		quota_name, 
    		quota_value,
    		last_quota_value, 
    		last_quota_date,
    		gmt_created, 
    		gmt_modifed 
    from b_oracle_quota_collect_day 
    	where gmt_created  <![CDATA[ <  ]]>  str_to_date(#{gmtCreatedDate},'%Y-%m-%d') 
  </insert>
 
</mapper>